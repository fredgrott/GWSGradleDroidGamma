<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.github.shareme.gwsgradledroidgamma">

    <!-- new android gradle 2.0 plugin and api checks minSDK declarations in
         deps and attempts to match up, to resolve those errors since
         AppCompat has minSk of 1 is to place a minSdkVersion declaration
         here to force it-->
